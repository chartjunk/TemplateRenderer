@{
    Layout = null;
}
<style>
	body {
		box-sizing: inherit;
		-webkit-font-smoothing: antialiased;
		height: 100%;
		width: 100%;
		padding: 0;
		color: rgba(0, 0, 0, 0.87);
		margin: 0;
		font-size: 0.875rem;
		font-family: "Roboto", "Helvetica", "Arial", sans-serif;
		font-weight: 400;
		line-height: 1.43;
		letter-spacing: 0.01071em;
		zoom: 60%;
	}
	#content {
		margin-right: 20px;
	}
</style>
<html>
<head>
    <title>Hello React</title>
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" />
</head>
<body>
    <div id="content"></div>
    <script crossorigin="true" src="https://cdnjs.cloudflare.com/ajax/libs/react/16.8.0/umd/react.development.js"></script>
    <script crossorigin="true" src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.8.0/umd/react-dom.development.js"></script>
    <script>		
        window['__entry'] = @Html.Raw(JsonConvert.SerializeObject(ViewBag.Entry));
	    @Html.Raw(ViewBag.Template);
    </script>
</body>
</html>